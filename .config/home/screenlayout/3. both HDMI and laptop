#!/usr/bin/env bash
xrandr \
  --output eDP-1 \
    --mode 1920x1080 \
    --pos 1920x0 \
    --rotate normal \
    --rate 165 \
  --output HDMI-1-0 \
    --mode 1920x1080 \
    --pos 0x0 \
    --rotate normal \
    --rate 165 \
  --output HDMI-1 --off \
  --output DP-1 --off \
  --output DP-2 --off
